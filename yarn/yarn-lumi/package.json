{
  "name": "lumi",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/lumi-media/web",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected:test:dev": " yarn nx affected:test --base=origin/development --parallel",
    "build": "ts-node --project tools/tsconfig.tools.json tools/build/build.ts --target=build:prod",
    "build:prod": "yarn && node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "build:share": "yarn && npx ng build external-share --configuration=production",
    "build:admin": "yarn && npx ng build admin --configuration=production",
    "build:casting": "yarn && node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build casting --configuration=production",
    "postbuild:casting": "mkdir -p dist/sourcemaps/casting; mv dist/apps/casting/*.map dist/sourcemaps/casting",
    "build:analyze": "yarn build --stats-json && npx webpack-bundle-analyzer dist/lumi/stats.json",
    "build:dev": "yarn && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=development",
    "build:schematics": "ts-node -P tools/tsconfig.tools.json tools/scripts/build-schematics ",
    "cache:clear": "npx rimraf node_modules/.cache .cache && npm cache verify",
    "dep-graph": "nx dep-graph",
    "docs": "nx run lumi:compodoc",
    "docker": "docker-compose -f ./.docker/docker-compose.yml up",
    "docker:build": "yarn docker --build",
    "e2e": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng e2e lumi-e2e --configuration=production",
    "e2e:admin": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng e2e admin-e2e --configuration=production",
    "e2e:admin:local": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng e2e admin-e2e --configuration=local",
    "e2e:dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng e2e lumi-e2e --configuration=development --watch",
    "e2e:local": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng e2e lumi-e2e --configuration=local --watch",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:lastcommit": "ts-node --project tools/tsconfig.json tools/utils/git-diff.ts",
    "json": "ts-node --project tools/tsconfig.json tools/generate-package-json/index.ts",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "lint": "nx workspace-lint && ng lint",
    "lint:fix": "yarn lint --fix",
    "nx": "nx",
    "prettify": "ts-node --project tools/tsconfig.tools.json tools/utils/prettify.ts",
    "s": "yarn schematic",
    "schematic": "nx workspace-schematic",
    "serve:admin:local": "yarn && npx ng serve admin --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=local",
    "serve:admin:dev": "yarn && npx ng serve admin --configuration=development",
    "serve:admin:release": "yarn && npx ng serve admin configuration=production",
    "serve:lumi": "ts-node --project tools/tsconfig.tools.json tools/build/build.ts --target=serve --project=lumi",
    "serve:ssl:share": "ng serve external-share --configuration=local --ssl --ssl-key localhost.key --ssl-cert localhost.crt",
    "serve:ssl:share:dev": "ng serve external-share --configuration=development --ssl --ssl-key localhost.key --ssl-cert localhost.crt",
    "serve:ssl:share:qa": "ng serve external-share --configuration=qa --ssl --ssl-key localhost.key --ssl-cert localhost.crt",
    "serve": "yarn && node --max_old_space_size=8192 ./node_modules/@nrwl/cli/bin/nx  serve",
    "serve:dev": "yarn serve:lumi --configuration=development",
    "serve:local": "yarn serve:lumi --configuration=local",
    "serve:ssl:local": "yarn serve:lumi --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=ssl-local",
    "serve:ssl:dev": "yarn serve:lumi --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=ssl-dev",
    "serve:qa": "yarn serve:lumi --configuration=qa",
    "serve:release": "yarn serve --configuration=production",
    "serve:release:dev": "yarn serve --configuration=release-dev",
    "serve:release:local": "yarn serve --configuration=release-local",
    "serve:casting": "yarn && npx ng serve casting --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=local",
    "serve:ssl:dev:fast": "yarn serve:lumi --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=fast-ssl-dev",
    "serve:ssl:local:fast": "yarn serve:lumi --ssl --ssl-key localhost.key --ssl-cert localhost.crt --configuration=fast-ssl-local",
    "serve:casting:dev": "yarn && npx ng serve casting --configuration=development",
    "serve:casting:release": "yarn && npx ng serve casting configuration=production",
    "serve:casting:release:dev": "yarn && npx ng serve casting --configuration=release-dev",
    "serve:casting:release:local": "yarn && npx ng serve casting configuration=release-local",
    "storybook": "nx run components:storybook",
    "test": "npx ng test",
    "test:all": "npx nx run-many --target=test --all",
    "update": "nx migrate latest",
    "update:check": "ng update"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  },
